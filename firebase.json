{
  "hosting": {
    "public": "dist\\angular-tour-of-heroes",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
	"rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/products/**",
        "destination": "/index.html"
      }
    ],
	"cleanUrls": true
  }
}
